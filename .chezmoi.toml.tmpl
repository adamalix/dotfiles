{{- $name := "" -}}
{{- if .name -}}
{{-   $name = .name -}}
{{- else -}}
{{-   $name = promptString "name" -}}
{{- end -}}

{{- $email := "" -}}
{{- if .email -}}
{{-   $email = .email -}}
{{- else -}}
{{-   $email = promptString "email" -}}
{{- end -}}

[data]
    email = "{{ $email }}"
    name = "{{ $name }}"
