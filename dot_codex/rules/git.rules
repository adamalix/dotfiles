prefix_rule(
    pattern = ["git", ["add", "checkout", "commit", "diff", "fetch", "push", "rebase", "stash", "status", "switch"]],
    decision = "allow",
    match = [["git", "stash", "push"], ["git", "add", "lmao.py"]],
    not_match = [["git", "merge"]],
)

prefix_rule(
    pattern = ["git", ["merge"]],
    decision = "prompt",
    match = [["git", "merge"]],
    not_match = [["git", "fart"], ["git", "checkout"], ["git", "diff"], ["git", "lullaby"]],
)
