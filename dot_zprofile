if [[ -n ${CODEX_SANDBOX:-} ]]; then
    # echo "Running under Codex (sandbox=$CODEX_SANDBOX)"
    # No need to eval the shellenv in the Codex sandbox
    exit 0
fi

eval "$(/opt/homebrew/bin/brew shellenv)"

if [[ -f $HOME/.zprofile.local ]]; then
	source $HOME/.zprofile.local || echo "Warning: Failed to load local zprofile configuration"
fi
